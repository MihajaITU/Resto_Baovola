<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListeProduit</servlet-name>
        <servlet-class>Control.ListeProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeType</servlet-name>
        <servlet-class>Control.ListeType</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeCommande</servlet-name>
        <servlet-class>Control.ListeCommande</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeProduit</servlet-name>
        <servlet-class>Control.TypeProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommndeParType</servlet-name>
        <servlet-class>Control.CommndeParType</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListePrixReviens</servlet-name>
        <servlet-class>Control.ListePrixReviens</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListeProduit</servlet-name>
        <url-pattern>/ListeProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeType</servlet-name>
        <url-pattern>/ListeType</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeCommande</servlet-name>
        <url-pattern>/ListeCommande</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeProduit</servlet-name>
        <url-pattern>/TypeProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommndeParType</servlet-name>
        <url-pattern>/CommndeParType</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListePrixReviens</servlet-name>
        <url-pattern>/ListePrixReviens</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
